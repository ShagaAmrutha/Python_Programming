class Student:
    def __init__(self,name,rollno,marks):
        self.name=name
        self.rollno=rollno
        self.marks=marks
    def display(self):
        print(f"Name: {self.name}, Roll No: {self.rollno}, Marks: {self.marks}")
std1 = Student("Alice",23,95)
std2 = Student("bob",45,88)
std1.display()
std2.display()
