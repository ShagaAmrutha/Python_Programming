class Student:
    def __init__(self,name,rollno,marks):
        self.__name=name
        self.__rollno=rollno
        self.__marks=marks
    def display(self):
        print(f"Name: {self.__name}, Roll No: {self.__rollno}, Marks: {self.__marks}")
std1 = Student("Alice",23,95)
std2 = Student("bob",45,88)
std1.display()
std2.display()
